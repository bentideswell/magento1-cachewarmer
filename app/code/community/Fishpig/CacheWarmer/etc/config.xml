<?xml version="1.0"?>
<config>
	<modules>
		<Fishpig_CacheWarmer>
			<version>1.1.1.9</version>
		</Fishpig_CacheWarmer>
	</modules>
	<global>
		<helpers>
			<cachewarmer>
				<class>Fishpig_CacheWarmer_Helper</class>
			</cachewarmer>
		</helpers>
		<models>
			<cachewarmer>
				<class>Fishpig_CacheWarmer_Model</class>
			</cachewarmer>
		</models>
	</global>
	<default>
		<cachewarmer>
			<settings>
				<enabled>1</enabled>
				<run_from_browser>1</run_from_browser>
				<sources>store_init,catalog_product,catalog_category,cms_pages,wordpress,custom</sources>
				<useragent>ua_default</useragent>
				<currency_codes_all>1</currency_codes_all>
				<catalog_product_visibility>2,4</catalog_product_visibility>
				<curl_multi_threads>10</curl_multi_threads>
			</settings>
		</cachewarmer>
	</default>
</config>